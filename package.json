{
    "name": "app-outlet",
    "version": "2.0.0",
    "scripts": {
        "ng": "ng",
        "start": "npm-run-all -p electron:serve ng:serve",
        "start:core": "node core",
        "ng:serve": "ng serve",
        "electron:serve": "wait-on tcp:4200 && npx electron . --serve",
        "build": "ng build",
        "test": "NODE_ENV=test TZ=utc jest",
        "test:coverage": "npm test -- --coverage",
        "test:quick": "npm test -- --no-coverage",
        "test:watch": "npm test -- --watch",
        "test:watch:quick": "npm run test:watch -- --no-coverage",
        "test:report": "open coverage/lcov-report/index.html",
        "e2e": "ng e2e",
        "format": "prettier --write .",
        "lint": "eslint {src,e2e}/**/*.ts core/**/*.js --fix",
        "check:style": "prettier --check ."
    },
    "main": "main.js",
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.1.1",
        "@angular/common": "~11.1.1",
        "@angular/compiler": "~11.1.1",
        "@angular/core": "~11.1.1",
        "@angular/forms": "~11.1.1",
        "@angular/platform-browser": "~11.1.1",
        "@angular/platform-browser-dynamic": "~11.1.1",
        "@angular/router": "~11.1.1",
        "axios": "^0.21.1",
        "loglevel": "^1.7.1",
        "ngx-electron": "^2.2.0",
        "rxjs": "~6.6.0",
        "sqlite3": "^5.0.2",
        "tslib": "^2.0.0",
        "typeorm": "^0.2.30",
        "zone.js": "~0.11.3"
    },
    "devDependencies": {
        "@angular-builders/jest": "^11.0.0",
        "@angular-devkit/build-angular": "~0.1101.2",
        "@angular/cli": "~11.1.2",
        "@angular/compiler-cli": "~11.1.1",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@types/jest": "^26.0.20",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "codelyzer": "^6.0.0",
        "electron": "^11.3.0",
        "electron-builder": "^22.9.1",
        "electron-reload": "^1.5.0",
        "eslint": "^7.19.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.1",
        "glob": "^7.1.6",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "jest-preset-angular": "^8.3.2",
        "lint-staged": "^10.5.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.1.2",
        "wait-on": "^5.2.1"
    }
}
